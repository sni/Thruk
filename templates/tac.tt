              <td valign="top">[% UNLESS command_disabled(c, '42') %]<a href='cmd.cgi?cmd_typ=42'>[% END %]<img src='[% url_prefix %]themes/[% theme %]/images/tacenabled.png' border="0" alt='Event Handlers Enabled' title='Event Handlers Enabled' width="20" height="76">[% UNLESS command_disabled(c, '42') %]</a>[% END %]</td>
              [% ELSIF pi.enable_event_handlers == -1 %]
              <td valign="top">[% UNLESS command_disabled(c, '42') %]<a href='cmd.cgi?cmd_typ=42'>[% END %]<img src='[% url_prefix %]themes/[% theme %]/images/tacdiverse.png' border="0" alt='Event Handlers Ambiguous' title='Event Handlers Ambiguous' width="20" height="76">[% UNLESS command_disabled(c, '42') %]</a>[% END %]</td>
              [% ELSE %]
              <td valign="top">[% UNLESS command_disabled(c, '41') %]<a href='cmd.cgi?cmd_typ=41'>[% END %]<img src='[% url_prefix %]themes/[% theme %]/images/tacdisabled.png' border="0" alt='Event Handlers Disabled' title='Event Handlers Disabled' width="20" height="76">[% UNLESS command_disabled(c, '41') %]</a>[% END %]</td>
              [% END %]
              <td width="10">&nbsp;</td>

              [% IF pi.keys.size > 0 && pi.enable_event_handlers == 0 %]
              <td valign="middle" width="100%" class='featureDisabledHandlers'>N/A</td>
              [% ELSE %]
              <td valign="top" width="100%" class='featureEnabledHandlers'>
                <table border="0" width="100%">
                  <tr>
                    [% IF empty || service_stats.eventhandler_disabled == 0 %]
                    <td width="100%" class='featureItemEnabledServiceHandlers'>All Services Enabled</td>
                    [% ELSE %]
                    <td width="100%" class='featureItemDisabledServiceHandlers'><a href='status.cgi?host=all&amp;type=detail&amp;serviceprops=64'>[% service_stats.eventhandler_disabled %] Service Disabled</a></td>
                    [% END %]
                  </tr>
                  <tr>
                    [% IF empty || host_stats.eventhandler_disabled == 0 %]
                    <td width="100%" class='featureItemEnabledHostHandlers'>All Hosts Enabled</td>
                    [% ELSE %]
                    <td width="100%" class='featureItemDisabledHostHandlers'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=64'>[% host_stats.eventhandler_disabled %] Hosts Disabled</a></td>
                    [% END %]
                  </tr>
                </table>
              </td>
              [% END %]
            </tr>
          </table>
        </td>
        <td valign="top">
          <table border="0" width="135" cellspacing="0" cellpadding="0">
            <tr>
              [% IF pi.keys.size == 0 || pi.execute_service_checks == 1 %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacenabled.png' border='0' alt='Active Checks Enabled' title='Active Checks Enabled' width="20" height="76"></a></td>
              [% ELSIF pi.execute_service_checks == -1 %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacdiverse.png' border='0' alt='Active Checks Ambiguous' title='Active Checks Ambiguous' width="20" height="76"></a></td>
              [% ELSE %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacdisabled.png' border='0' alt='Active Checks Disabled' title='Active Checks Disabled' width="20" height="76"></a></td>
              [% END %]
              <td width="10">&nbsp;</td>
              [% IF pi.keys.size > 0 && pi.execute_service_checks == 0 %]
              <td valign="middle" width="100%" class='featureDisabledActiveChecks'>N/A</td>
              [% ELSE %]
              <td valign="top" width="100%" class='featureEnabledActiveChecks'>
                <table border="0" width="100%">
                  [% IF empty || service_stats.active_checks_disabled_active + service_stats.active_checks_disabled_passive == 0 %]
                  <tr><td width="100%" class='featureItemEnabledActiveServiceChecks'>All Services Enabled</td></tr>
                  [% ELSE %]
                    [% IF strict_passive_mode %]
                      [% IF service_stats.active_checks_disabled_active > 0 %]<tr><td width="100%" class='featureItemDisabledActiveServiceChecks'><a href='status.cgi?host=all&amp;type=detail&amp;serviceprops=131088'>[% service_stats.active_checks_disabled_active %] Services Disabled</a></td></tr>[% END %]
                      [% IF service_stats.active_checks_disabled_passive > 0 %]<tr><td width="100%" class='featureItemEnabledActiveServiceChecks'><a href='status.cgi?host=all&amp;type=detail&amp;serviceprops=65552'>[% service_stats.active_checks_disabled_passive %] Services Passive</a></td></tr>[% END %]
                    [% ELSE %]
                      <tr><td width="100%" class='featureItemDisabledActiveServiceChecks'><a href='status.cgi?host=all&amp;type=detail&amp;serviceprops=16'>[% service_stats.active_checks_disabled_active + service_stats.active_checks_disabled_passive %] Services Disabled</a></td></tr>
                    [% END %]
                  [% END %]

                  [% IF empty || host_stats.active_checks_disabled_active + host_stats.active_checks_disabled_passive == 0 %]
                  <tr><td width="100%" class='featureItemEnabledActiveHostChecks'>All Hosts Enabled</td></tr>
                  [% ELSE %]
                    [% IF strict_passive_mode %]
                      [% IF host_stats.active_checks_disabled_active > 0 %]<tr><td width="100%" class='featureItemDisabledActiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=131088'>[% host_stats.active_checks_disabled_active %] Hosts Disabled</a></td></tr>[% END %]
                      [% IF host_stats.active_checks_disabled_passive > 0 %]<tr><td width="100%" class='featureItemEnabledActiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=65552'>[% host_stats.active_checks_disabled_passive %] Hosts Passive</a></td></tr>[% END %]
                    [% ELSE %]
                      <tr><td width="100%" class='featureItemDisabledActiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=16'>[% host_stats.active_checks_disabled_active + host_stats.active_checks_disabled_passive %] Hosts Disabled</a></td></tr>
                    [% END %]
                  [% END %]
                </table>
              </td>
              [% END %]
            </tr>
          </table>
        </td>
        <td valign="top">
          <table border="0" width="135" cellspacing="0" cellpadding="0">
            <tr>
              [% IF pi.keys.size == 0 || pi.accept_passive_service_checks == 1 %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacenabled.png' border='0' alt='Passive Checks Enabled' title='Passive Checks Enabled' width="20" height="76"></a></td>
              [% ELSIF pi.accept_passive_service_checks == -1 %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacdiverse.png' border='0' alt='Passive Checks Ambiguous' title='Passive Checks Ambiguous' width="20" height="76"></a></td>
              [% ELSE %]
              <td valign="top"><a href='extinfo.cgi?type=0'><img src='[% url_prefix %]themes/[% theme %]/images/tacdisabled.png' border='0' alt='Passive Checks Disabled' title='Passive Checks Disabled' width="20" height="76"></a></td>
              [% END %]
              <td width="10">&nbsp;</td>
              [% IF pi.keys.size > 0 && pi.accept_passive_service_checks == 0 %]
              <td valign="middle" width="100%" class='featureDisabledPassiveChecks'>N/A</td>
              [% ELSE %]
              <td valign="top" width="100%" class='featureEnabledPassiveChecks'>
                <table border="0" width="100%">
                  <tr>
                    [% IF empty || service_stats.passive_checks_disabled == 0 %]
                    <td width="100%" class='featureItemEnabledPassiveServiceChecks'>All Services Enabled</td>
                    [% ELSE %]
                    <td width="100%" class='featureItemDisabledPassiveServiceChecks'><a href='status.cgi?host=all&amp;type=detail&amp;serviceprops=16384'>[% service_stats.passive_checks_disabled %] Service Disabled</a></td>
                    [% END %]
                  </tr>
                  <tr>
                    [% IF empty || host_stats.passive_checks_disabled == 0 %]
                    <td width="100%" class='featureItemEnabledPassiveHostChecks'>All Hosts Enabled</td>
                    [% ELSE %]
                    <td width="100%" class='featureItemDisabledPassiveHostChecks'><a href='status.cgi?host=all&amp;style=hostdetail&amp;hostprops=16384'>[% host_stats.passive_checks_disabled %] Host Disabled</a></td>
                    [% END %]
                  </tr>
                </table>
              </td>
              [% END %]
            </tr>
          </table>
        </td>
      </tr>
    </table>
</main>
